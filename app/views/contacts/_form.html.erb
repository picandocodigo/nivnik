<%= form_for :contact, url: contacts_path do |f| %>
<p>
  <%= f.label :name %><br/>
  <%= f.text_field :name %>
</p>

<p>
  <%= f.label :surname %><br/>
  <%= f.text_field :surname %>
</p>

<p>
  <%= f.label :description %><br/>
  <%= f.text_field :description %>
</p>

<p>
  <%= f.label :phone_number %><br/>
  <%= f.text_field :phone_number %>
</p>

<p>
  <%= f.label :email %><br/>
  <%= f.text_field :email %>
</p>

<p>
  <%= f.label :notes %><br/>
  <%= f.text_field :notes %>
</p>

<%= f.submit %>

<% end %>
